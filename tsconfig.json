{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "module": "es6",
    "target": "es5",
    "allowJs": true,
    "checkJs": true,
    "moduleResolution": "node"
  },
  "include": ["**/*.js", "**/*.ts",],
  "exclude": ["dist", "node_modules"],
}